{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext", "es5", "es6"],
    "jsx": "react-jsx",
    "useDefineForClassFields": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
    }
  },
  "files": [
    "src/infini-react-add-to-calendar.tsx"
  ],
  "include": [
    "src/**/*", "decs.d.ts"
  ],
  "exclude": [
    "src/**/*.test*"
  ],
  "types": []
}


